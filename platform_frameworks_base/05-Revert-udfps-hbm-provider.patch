 import com.android.wm.shell.bubbles.Bubbles;
 
 import java.util.Optional;
@@ -176,17 +175,8 @@ public abstract class SystemUIModule {
     @BindsOptionalOf
     abstract StatusBar optionalStatusBar();
 
